{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-countries-customer-data"
		},
		"ADLSGen2Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2Storage'"
		},
		"SQLDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLDatabase'"
		},
		"ADLSGen2Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stcountriesdata.dfs.core.windows.net/"
		},
		"RestCountriesAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/name/"
		},
		"SQLDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-internship-countries.database.windows.net"
		},
		"SQLDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "CustomerProductDB"
		},
		"SQLDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-countries-customer-data_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2Storage_accountKey": {
						"value": "[parameters('ADLSGen2Storage_accountKey')]"
					},
					"SQLDatabase_password": {
						"value": "[parameters('SQLDatabase_password')]"
					},
					"ADLSGen2Storage_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2Storage_properties_typeProperties_url')]"
					},
					"RestCountriesAPI_properties_typeProperties_url": {
						"value": "[parameters('RestCountriesAPI_properties_typeProperties_url')]"
					},
					"SQLDatabase_properties_typeProperties_server": {
						"value": "[parameters('SQLDatabase_properties_typeProperties_server')]"
					},
					"SQLDatabase_properties_typeProperties_database": {
						"value": "[parameters('SQLDatabase_properties_typeProperties_database')]"
					},
					"SQLDatabase_properties_typeProperties_userName": {
						"value": "[parameters('SQLDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}